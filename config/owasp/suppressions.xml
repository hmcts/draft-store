  </suppress>
  <suppress>
    <notes><![CDATA[
     Suppressing as it seems a false positive
   ]]></notes>
    <gav regex="true">^org\.springframework\.security:spring-security-crypto:5.1.4.RELEASE</gav>
    <cpe>cpe:/a:pivotal_software:spring_security</cpe>
